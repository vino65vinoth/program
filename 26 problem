 const prompt = require ("prompt-sync")();

 const mark = prompt ("totalmark");
 const totalmark = parseInt (mark);

 if (totalmark == 100) {
    console.log ("A grade")
 }
 else if (totalmark >= 90) {
    console.log ("B grade")
 }
 else if (totalmark >= 70) {
    console.log ("C grade")
 }
 else if (totalmark >= 35) {
    console.log ("D grade")
 }
 else  {
    console.log ("fail")
 }
     
 
